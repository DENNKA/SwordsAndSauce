[gd_scene load_steps=5 format=2]

[sub_resource type="GDScript" id=5]
script/source = "extends Area

export var action_name = ''
export var action_id = ''

export var delete_parent = true
export var delete_on_action = true
export var parent_function = ''

func action():
	if action_id:
		Global.pack[action_id] = {
			'name' : action_name
		}
	
	if parent_function:
		get_parent().call(parent_function)
	
	if delete_parent:
		get_parent().queue_free()
	elif delete_on_action:
		queue_free()


func _on_Area_body_shape_entered(body_rid, body, body_shape_index, local_shape_index):
	if body.name == \"player\":
		print(\"wdwa\")
		action()

"

